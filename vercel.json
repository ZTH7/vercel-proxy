{
   "rewrites":[
      {
         "source":"/:match/:url*",
         "destination":"https://:match/:url*",
         "has":[
            {
               "type":"header",
               "key":"x-forwarded-proto",
               "value":"https"
            }
         ]
      },
      {
         "source":"/:match/:url*/",
         "destination":"https://:match/:url*/",
         "has":[
            {
               "type":"header",
               "key":"x-forwarded-proto",
               "value":"https"
            }
         ]
      },
      {
         "source":"/:match/:url*",
         "destination":"http://:match/:url*",
         "has":[
            {
               "type":"header",
               "key":"x-forwarded-proto",
               "value":"http"
            }
         ]
      },
      {
         "source":"/:match/:url*/",
         "destination":"http://:match/:url*/",
         "has":[
            {
               "type":"header",
               "key":"x-forwarded-proto",
               "value":"http"
            }
         ]
      }
   ]
}
